{	"#each": {
		"prefix": "each",
		"body": [
			"{#each ${1:items}}",
			"\t{it.${2:name}}$0",
			"{/each}"
		],
		"description": "Loop section with implicit alias"
	},
	"#eval": {
		"prefix": "eval",
		"body": [
			"{#eval /}"
		],
		"description": "Parse and evaluate a template dynamically"
	},
	"#for": {
		"prefix": "for",
		"body": [
			"{#for ${1:item} in ${2:items}}",
			"\t{${1:item}.${3:name}}$0",
			"{/for}"
		],
		"description": "Loop section with alias"
	},
	"#if": {
		"prefix": "if",
		"body": [
			"{#if ${1:condition}}",
			"\t$0",
			"{/if}"
		],
		"description": "If section"
	},
	"#include": {
		"prefix": "include",
		"body": [
			"{#include ${1:base}}",
			"\t$0",
			"{/include}"
		],
		"description": "Include section"
	},
	"#insert": {
		"prefix": "insert",
		"body": [
			"{#insert ${1:body}}",
			"\t$0",
			"{/insert}"
		],
		"description": "Insert section"
	},
	"#with": {
		"prefix": "with",
		"body": [
			"{#with ${1:item}}",
			"\t{${2:name}}$0",
			"{/with}"
		],
		"description": "With section"
	},
	"#with-as": {
		"prefix": "withas",
		"body": [
			"{#with ${1:item} as ${2:alias}}",
			"\t{${2:alias}.${3:name}}$0",
			"{/with}"
		],
		"description": "With section with alias"
	}
}
